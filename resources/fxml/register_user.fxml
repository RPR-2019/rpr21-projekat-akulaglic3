<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<GridPane hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.Controllers.RegisterUserController">
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <children>
        <Label text="Enter you information:" GridPane.columnSpan="2">
            <font>
                <Font name="Times New Roman" size="14.0" />
            </font>
        </Label>
        <Label text="First name:" GridPane.rowIndex="1" />
        <Label text="Second name:" GridPane.rowIndex="2" />
        <Label text="Username:" GridPane.rowIndex="3" />
        <Label text="Password:" GridPane.rowIndex="4" />
        <Label text="Email:" GridPane.rowIndex="5" />
        <TextField fx:id="fldName" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1" />
        <TextField fx:id="fldSurname" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
        <PasswordField fx:id="fldPassword" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="4" />
        <TextField fx:id="fldDoctorName" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="6" />
        <TextField fx:id="fldEmail" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="5" />
        <ButtonBar GridPane.columnIndex="4" GridPane.columnSpan="2" GridPane.rowIndex="9">
            <buttons>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#actionRegister" text="Register" />
                <Button mnemonicParsing="false" onAction="#actionBack" text="Back" />
            </buttons>
        </ButtonBar>
      <Label prefHeight="17.0" prefWidth="119.0" text="Doctor name:" GridPane.rowIndex="6" />
      <Label text="Doctor surname:" GridPane.rowIndex="7" />
      <TextField fx:id="fldUsername" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="3" />
      <TextField fx:id="fldDoctorSurname" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="7" />
      <Label text="Check the boxes of allergies that apply to you:" GridPane.columnIndex="3" GridPane.columnSpan="3" />
      <Line endY="170.0" startY="-70.0" GridPane.columnIndex="3" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets />
         </GridPane.margin></Line>
      <CheckBox fx:id="cbAspirin" mnemonicParsing="false" text="Allergy to Aspirin" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></CheckBox>
      <CheckBox fx:id="cbAntibiotics" mnemonicParsing="false" text="Allergy to Antibiotics" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></CheckBox>
      <CheckBox fx:id="cbNID" mnemonicParsing="false" text="Allergy to non-steroidal inflammatory drugs" GridPane.columnIndex="3" GridPane.columnSpan="3" GridPane.rowIndex="3">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></CheckBox>
      <CheckBox fx:id="cbSulfa" mnemonicParsing="false" text="Allergy to sulfa drugs" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></CheckBox>
      <CheckBox fx:id="cbChem" mnemonicParsing="false" text="Allergy to chemotherapy drugs" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="5">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></CheckBox>
      <CheckBox fx:id="cbInsulin" mnemonicParsing="false" text="Allergy to insulin" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="6">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></CheckBox>
      <CheckBox fx:id="cbAntiS" mnemonicParsing="false" text="Allergy to anti-seizure drugs" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="7">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></CheckBox>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
    <padding>
        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
    </padding>
</GridPane>
